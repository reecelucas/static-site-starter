{
    "name": "static-site-starter",
    "version": "1.0.0",
    "description": "Simple static site starter using Eleventy",
    "main": "n/a",
    "author": "Reece Lucas <reecelucas@sky.com>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/reecelucas/static-site-starter.git"
    },
    "keywords": [
        "static",
        "skeleton",
        "starter",
        "boilerplate",
        "template",
        "performance",
        "es2015",
        "Gulp",
        "Babel",
        "Nunjucks",
        "Eleventy",
        "JAM Stack"
    ],
    "scripts": {
        "clean": "rimraf dist/*",
        "start": "yarn clean && gulp",
        "build": "yarn clean && gulp build --env production",
        "build:server": "yarn clean && gulp build:server --env production",
        "lint": "eslint 'src/js/**/*.js' & stylelint 'src/styles/**/*.scss'",
        "format": "prettier --write 'src/js/**/*.js'",
        "test": "jest",
        "test:coverage": "jest --coverage --collectCoverageFrom 'src/js/**/*.js'",
        "precommit": "yarn lint && yarn format"
    },
    "dependencies": {
        "babel-polyfill": "^6.26.0",
        "fontfaceobserver": "^2.0.13"
    },
    "devDependencies": {
        "@babel/register": "^7.0.0-beta.51",
        "autoprefixer": "^8.0.0",
        "babel-cli": "^6.26.0",
        "babel-plugin-external-helpers": "^6.22.0",
        "babel-preset-env": "^1.7.0",
        "browser-sync": "^2.24.4",
        "camelcase": "^5.0.0",
        "css-mqpacker": "^6.0.1",
        "eslint": "^4.19.1",
        "eslint-config-airbnb": "^16.1.0",
        "eslint-config-prettier": "^2.9.0",
        "eslint-plugin-import": "^2.12.0",
        "eslint-plugin-jsx-a11y": "^6.0.3",
        "eslint-plugin-react": "^7.9.1",
        "glob": "^7.1.2",
        "gulp": "^3.9.1",
        "gulp-buffer": "^0.0.2",
        "gulp-cli": "^2.0.1",
        "gulp-htmlmin": "^4.0.0",
        "gulp-if": "^2.0.2",
        "gulp-imagemin": "^4.1.0",
        "gulp-inline-source": "^3.1.0",
        "gulp-postcss": "^7.0.1",
        "gulp-purgecss": "^1.0.0",
        "gulp-sass": "^4.0.1",
        "gulp-shell": "^0.6.5",
        "gulp-sourcemaps": "^2.6.4",
        "gulp-uglify": "^3.0.0",
        "jest": "^23.1.0",
        "merge-stream": "^1.0.1",
        "minimist": "^1.2.0",
        "path": "^0.12.7",
        "portscanner": "^2.2.0",
        "postcss-discard-duplicates": "^2.1.0",
        "prettier": "^1.13.5",
        "rollup": "^0.60.7",
        "rollup-plugin-babel": "^3.0.4",
        "rollup-plugin-commonjs": "^8.3.0",
        "rollup-plugin-node-resolve": "^3.3.0",
        "rollup-stream": "^1.24.1",
        "run-sequence": "^2.2.1",
        "sass-mq": "^5.0.0",
        "stylelint": "^9.3.0",
        "stylelint-scss": "^3.1.2",
        "vinyl-source-stream": "^2.0.0",
        "workbox-build": "^3.3.0"
    }
}
